{
  "name": "Square Root",
  "description": "Calculates the number which, when multiplied by itself, will give the cell value.",
  "tags": [
    "preprocessing",
    "transformation"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/sqrt_operator"
  ],
  "container": "ghcr.io/tercen/sqrt_operator:specs",
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Values",
                "type": "numeric",
                "description": "Values to calculate square root of. Multiple values per cell will be averaged before calculation.",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}